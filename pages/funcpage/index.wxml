<!--pages/funcpage/index.wxml-->
<scroll-view scroll-x class="bg-white nav">
  <view class="flex text-center">

    <view class="cu-item flex-sub {{index==TabCur?'text-blue cur':''}}" wx:for="{{tabData}}" wx:key bindtap="tabSelect" data-id="{{index}}">
      {{item}}
    </view>
  </view>
</scroll-view>

<realtimedata wx:if="{{TabCur==0}}"></realtimedata>

<historydata wx:if="{{TabCur==1}}"></historydata>

<ventilationrecord wx:if="{{TabCur==2}}"></ventilationrecord>

<optworkorder wx:if="{{TabCur==3}}"></optworkorder>

<deviceinfo wx:if="{{TabCur==4}}"></deviceinfo>