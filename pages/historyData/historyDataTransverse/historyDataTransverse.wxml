      </view>
    </view>

    <view class='right-center'>
      <view class="right-center-top">
        <text class='icon-fold text-orange'></text>
      </view>
      <view class="right-center-center">
        <text>2019-03-14 19</text>
      </view>
      <view class="right-center-bottom">
        <text class='icon-unfold text-orange'></text>
      </view>
    </view>

    <view class='right-bottom'>
      <view class="right-bottom-top" bindtap='navigateBack'>
        <text class='icon-close text-gary' style='font-size:60rpx;'></text>
      </view>

    </view>

  </view>
</view>
