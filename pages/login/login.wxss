/* pages/login/login.wxss */

page {
  height: 100%;
}

.warp {
  height: 100%;
  position: relative;
  overflow-y:hidden;
}

.warp image {
  width: 100%;
  height: 100%;
}

.buttonClass {
  width: 70%;
  margin: auto;
  margin-top: 70px;
  line-height: 50px;
}
.cu-btn.lg{
  height: 45px;
}
.phoneClass {
  width: 70%;
  margin: auto;
  margin-top: -95%;
}

.cu-form-group .title {
  font-size: 16px;
  color: #666;
}

.imageClass {
  z-index: -9999;
}

.lableClass {
  width: 25%;
  float: left;
  background-color: white;
  height: 45px;
  line-height: 45px;
  color: #666666;
  padding-left: 12px;
  font-size: 15px;
  border-bottom-left-radius: 4px;
  border-top-left-radius: 4px;

}

.inputClass {
  width: 75%;
  float: left;
  background-color: white;
  height: 45px;
  font-size: 15px;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
}
