<view class="cu-list menu">
  <view wx:if="{{resultData.length==0}}" class='container' style='height:82vh;text-align:center;padding-top:50%;color:#ccc;'>
            <text>暂无数据</text>
          </view>
  <view class="cu-item arrow itemClass" wx:for="{{resultData}}" wx:key bindtap="navigateChart" data-dgimn='{{item["dbo.T_Bas_QCAnalyzerControlCommand.DGIMN"]}}' data-pointname='{{item["dbo.T_Bas_CommonPoint.PointName"]}}'data-qcacontrolresultid='{{item["dbo.T_Bas_QCAControlResult.ID"]}}'data-qcresult='{{item["dbo.T_Bas_QCAControlResult.QCResult"]}}'data-qcresultname='{{item["dbo.T_Bas_QCAControlResult.QCResult_Name"]}}' data-qcanalyzerControlCommandid='{{item["dbo.T_Bas_QCAnalyzerControlCommand.ID"]}}' data-qcamn='{{item["dbo.T_Bas_QCAnalyzerControlCommand.QCAMN"]}}' data-qcexecutype='{{item["dbo.T_Bas_QCAnalyzerControlCommand.QCExecuType"]}}' data-qcexecutypename='{{item["dbo.T_Bas_QCAnalyzerControlCommand.QCExecuType_Name"]}}' data-qctime='{{item["dbo.T_Bas_QCAnalyzerControlCommand.QCTime"]}}' data-qctype='{{item["dbo.T_Bas_QCAnalyzerControlCommand.QCType"]}}' data-qctypename='{{item["dbo.T_Bas_QCAnalyzerControlCommand.QCType_Name"]}}' data-standardpollutantcode='{{itemF["dbo.T_Bas_QCAnalyzerControlCommand.StandardPollutantCode"]}}' data-standardpollutantname='{{item["dbo.T_Bas_QCAnalyzerControlCommand.StandardPollutantName"]}}' data-stoptime='{{item["dbo.T_Bas_QCAnalyzerControlCommand.StopTime"]}}' data-qcanalyzerinfoid='{{item["dbo.T_Bas_QCAnalyzerInfo.ID"]}}' data-qcaname='{{item["dbo.T_Bas_QCAnalyzerInfo.QCAName"]}}'>
    <view class="cu-avatar round lg" wx:if='{{item["dbo.T_Bas_QCAnalyzerControlCommand.StandardPollutantCode"]=="02"}}' style="background-image:url(/images/so2.png);"></view>
    <view class="cu-avatar round lg" wx:if='{{item["dbo.T_Bas_QCAnalyzerControlCommand.StandardPollutantCode"]=="03"}}' style="background-image:url(/images/nox.png);"></view>
    <view class="cu-avatar round lg" wx:if='{{item["dbo.T_Bas_QCAnalyzerControlCommand.StandardPollutantCode"]=="s01"}}' style="background-image:url(/images/o.png);"></view>
    <view class='content textClass'>
      <view class='text-grey'>{{item["dbo.T_Bas_QCAnalyzerControlCommand.StandardPollutantName"]}}质控</view>
      <view class='text-gray text-sm'>
        <text class='icon-infofill text-red'></text> {{item["dbo.T_Bas_CommonPoint.PointName"]}}的{{item["dbo.T_Bas_QCAnalyzerInfo.QCAName"]}}在{{item["dbo.T_Bas_QCAnalyzerControlCommand.QCTime"]}}到{{item["dbo.T_Bas_QCAnalyzerControlCommand.StopTime"]}}进行了{{item["dbo.T_Bas_QCAnalyzerControlCommand.StandardPollutantName"]}}的{{item["dbo.T_Bas_QCAnalyzerControlCommand.QCExecuType_Name"]}}质控
      </view>
    </view>
    <image wx:if="{{item['dbo.T_Bas_QCAControlResult.QCResult']==1}}" mode='widthFix' src="/images/hege.png" class="renqi"></image>
    <image wx:if="{{item['dbo.T_Bas_QCAControlResult.QCResult']==2}}" mode='widthFix' src="/images/buhege.png" class="renqi"></image>

  </view>
</view>