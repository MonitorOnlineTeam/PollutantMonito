.main{
  
  background: #F8F8F8;
}
/* 导航栏 */
.navigation {
  width: 100%;
  height: 83rpx;
  padding-left: 20rpx;
  padding-top: 5vh;
  background: #56a6ff;
    border: 1px #56a6ff solid;
    display: flex;
}
/* 导航栏标题 */
.navigation-title {
  color: #fff;
  font-size: 34rpx;
  font-weight: 400;
}
/* 上标题卡片 */
.titlecard {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 164rpx;
  background: #56a6ff;
}
/* 时间选择 */
.titlecard-datetime {
  
  width: 100%;
  display: flex;
  flex-direction: row;
}
/* 时间选择 */
.titlecard-datetime-time {
  width: 60%;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
}
/* 时间选择分布 */
.titlecard-datetime-box {
  width: 20%;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
}
/* 时间选择空间 */
.datetime-input {
  width: 100%;
  height: 48rpx;
  color: #56a6ff;
  font-size: 25rpx;
  line-height: 42rpx;
}
/* 图标 */
.titlecard-datetime-Arrow {
  height: 34rpx;
  width: 42rpx;
}
/* 多列选择器 */
.input-picker {
  width: 100%;
}
/* 污染物选择卡片 */
.titlecard-polltype {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}
/* 污染物选择图标 */
.titlecard-polltype-img {
  height: 18rpx;
  width: 20rpx;
}
/* 污染物选择 */
.select-polltype {
  width: 30%;
  height: 48rpx;
  color: #fff;
  font-size: 25rpx;
  line-height: 42rpx;
  background: #56a6ff;
  border: 1px #fff solid;
  border-radius: 20px;
}
/* tab标签 */
.tab-view {
  background:#FFF;
  width: 100%;
  display: flex;
  flex-direction: row;
  padding-top: 5px;
 justify-content:center;
}
/* tab标签 */
.tab-view-box{
width: 80%;
  display: flex;
  flex-direction: row;
 border-radius: 5px;
 justify-content:center;
   border-bottom: #56a6ff 1px solid;
  border-top: #56a6ff 1px solid;
  border-left: #56a6ff 0px solid;
  border-right: #56a6ff 1px solid;

}
/* tab标签 */
.tab-view-i {
  width: 25%;
  height: 50rpx;
  display: flex;
  border-bottom: #56a6ff 0px solid;
  border-top: #56a6ff 0px solid;
  border-left: #56a6ff 1px solid;
  border-right: #56a6ff 0px solid;
  align-items: center;
  flex-direction: column;
  font-size: 28rpx;
  color: #56a6ff;
  line-height: 50rpx;
}
/* tab标签 */
.tab-view-i-Left{
    width: 25%;
  height: 50rpx;
  display: flex;
  border-bottom: #56a6ff 0px solid;
  border-top: #56a6ff 0px solid;
  border-left: #56a6ff 1px solid;
  border-right: #56a6ff 0px solid;
  align-items: center;
  flex-direction: column;
  font-size: 28rpx;
  color: #56a6ff;
  line-height: 50rpx;
   border-radius: 5px 0px 0px 5px;
}
/* tab标签 */
.tab-view-select{
background: #56a6ff;
color: #fff;
}
/* 图表大小 */
.canvas{
  background:#FFF;
  width: 100vh;
  height: 35vh;
}
/* 列表 */
.list-view{
  margin-top: 15.6rpx;
width: 100%;

display: flex;
flex-direction: column;
  justify-content: space-around;
  align-items: center;
  background:#FFF;
}
.list-title-item{
width: 100%;
height: 90rpx;
background: #56a6ff;
display: flex;
flex-direction:  row;
  justify-content: space-around;  
    align-items: center;  
     font-size: 26rpx;
      color: #9C9C9C;
}
.list-item-title{
  width: 30%;
  text-align: center;

  
}
.list-item {

width: 100%;
display: flex;
flex-direction:  row;
  justify-content: space-around;  
    align-items: center;  
     font-size: 26rpx;
      color: #9C9C9C;
      
     
}
.list-item-text{
  width: 30%;
  text-align: center;


}
